{
	"manifest_version": 2,
	"name": "Прилипающий блок",
	"version": "1.0.0",
	"content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": ["jquery-git.min.js", "content.js"]
	  }
	],
	"browser_action": {
	  "default_icon": "images/icon.png",
      "default_title": "Тестирование разработки расширения",      // optional; shown in tooltip
      "default_popup": "popup.html"        // optional
	}
}